{
  "name": "kiko",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://yuriMalakhov.github.io/kiko/",
  "dependencies": {
    "cypress": "3.6.1",
    "lodash.template": "^4.5.0",
    "mixin-deep": "^1.3.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.2.0",
    "set-value": "^2.0.1",
    "start-server-and-test": "1.10.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test --env=jsdom",
    "cy:run": "cypress run --record --key 75a6dcf1-c9e8-4caf-81de-643915f11823",
    "cy:ci:test": "start-server-and-test start http-get://localhost:3000 cy:run",
    "test:coverage": "npm test -- --coverage",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "2.1.1"
  }
}
